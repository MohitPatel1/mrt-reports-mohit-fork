{
  "name": "@mohit11/smart-table",
  "version": "0.1.0",
  "description": "A modular, fully customizable TypeScript-based React component library that wraps and extends Material React Table (MRT)",
  "keywords": [
    "react",
    "material-ui",
    "table",
    "material-react-table",
    "mui",
    "typescript",
    "smart-table",
    "data-grid"
  ],
  "author": "Mohit",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "check": "biome check --write .",
    "ci:check": "biome ci ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mohit11/mrt-reports-mohit-fork"
  },
  "bugs": {
    "url": "https://github.com/mohit11/mrt-reports-mohit-fork/issues"
  },
  "homepage": "https://github.com/mohit11/mrt-reports-mohit-fork#readme",
  "peerDependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^6.0.0",
    "@mui/material": "^6.0.0",
    "@mui/x-date-pickers": "^7.15.0",
    "material-react-table": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-window": "^1.8.10"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^6.0.0",
    "@mui/material": "^6.0.0",
    "@mui/x-date-pickers": "^7.15.0",
    "@types/node": "^22.14.1",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.2.1",
    "material-react-table": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-window": "^1.8.10",
    "typescript": "~5.3.3",
    "vite": "^5.1.1",
    "vite-plugin-dts": "^4.5.3"
  }
}
